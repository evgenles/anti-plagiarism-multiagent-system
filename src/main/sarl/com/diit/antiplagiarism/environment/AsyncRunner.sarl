package com.diit.antiplagiarism.environment

import io.sarl.lang.core.EventListener
import io.sarl.lang.core.Event

 class AsyncRunner implements Runnable {

	var ^agent: EventListener
	var ^event: Event
	
	
	 new(^agent:EventListener, ^event: Event) {	
		this.^agent = ^agent
		this.^event = ^event
	}
	
	@Override
	def run() {
		this.^agent.receiveEvent(this.^event )
	}
	
	@Override
	def toString():String {
		return "[agent=" + this.^agent + "; event=" + this.^event + "]"
	}
}